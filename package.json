{
  "name": "react-tailwind-boilerplate",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "^6.2.0",
    "axios": "^0.21.1",
    "postcss": "7.0.36",
    "react": "^18.0.0-alpha-9eb2aaaf8-20210818",
    "react-dom": "^18.0.0-alpha-9eb2aaaf8-20210818",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.7",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "craco start",
    "dev": "yarn install && craco start",
    "build": "craco --max_old_space_size=10240 build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "analyze": "SET \"GENERATE_SOURCEMAP=true\" && yarn build && source-map-explorer 'build/static/js/*.js'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "node-sass": "4.14.1",
    "source-map-explorer": "^2.5.2"
  }
}
